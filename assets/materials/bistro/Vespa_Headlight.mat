pipelines:
    - main_probelit.vert.glsl main_probelit_nossr.frag.glsl
    - main_probelit.vert.glsl main_probelit_nossr.frag.glsl
flags:
    - two_sided
    - alpha_blend
textures:
    - bistro/Vespa_Headlight_BaseColor.tex
    - bistro/Vespa_Headlight_Normal.tex
    - bistro/Vespa_Headlight_roughness.tex
    - bistro/Vespa_Headlight_metallic.tex
params:
    - 1.0 1.0 1.0 1.0    # base color, roughness
    - 0.0 0.0 0.5 0.0    # sheen, sheen_tint, specular, spec_tint
    - 1.0 0.0 0.0 0.0    # metallic, transmission, clearcoat, clearcoat_roughness
    - 0.8999999985098839 0.0 0.0 0.0    # transparency, unused, unused, unused
