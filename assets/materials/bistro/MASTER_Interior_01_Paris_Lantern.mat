pipelines:
    - main_probelit.vert.glsl main_probelit_nossr.frag.glsl
    - main_probelit.vert.glsl main_probelit_nossr.frag.glsl
flags:
    - two_sided
textures:
    - bistro/MASTER_Interior_01_Paris_Lantern_BaseColor.tex
    - default_normalmap.dds
    - bistro/MASTER_Interior_01_Paris_Lantern_roughness.tex
    - bistro/MASTER_Interior_01_Paris_Lantern_metallic.tex
params:
    - 1.0 1.0 1.0 1.0    # base color, roughness
    - 0.0 0.0 0.5 0.0    # sheen, sheen_tint, specular, spec_tint
    - 1.0 0.0 0.0 0.0    # metallic, transmission, clearcoat, clearcoat_roughness
    - 0.0 0.0 0.0 0.0    # transparency, ior, unused, unused
